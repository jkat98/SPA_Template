@using System.Web.Optimization
<!DOCTYPE html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<!-- <html lang="en"> -->
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Website Title</title>
    <link href="~/Content/reset.css" rel="stylesheet" media="screen" />
    <link href="~/Content/styles.css" rel="stylesheet" media="screen" />
    <script src="~/Scripts/lib/modernizr.custom.js"></script>
</head>
<body>
    <div>
        <b>Single Page Application:</b>        

        @RenderPage("Views/sample.cshtml")
    </div>


    <!--
    When bundling, load in this sequence:
    1) shivs and jQuery
    2) 3rd party libraries
    3) require.js
    4) your app libraries
    5) main.js 
    -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="~/Scripts/lib/moment.min.js"></script>
    <script src="~/Scripts/lib/underscore.min.js"></script>
    <script src="~/Scripts/lib/backbone.min.js"></script>
    <script src="~/Scripts/lib/require.js"></script>
    <script src="~/Scripts/main.js"></script>

    @*
    <!-- If using Bundling, replace above code with custom version of below and setup BundleConfig.cs in App_Start -->
    @Scripts.Render(
        "~/bundles/jquery", 
        "~/bundles/jsextlibs", 
        "~/Scripts/lib/require.js", 
        "~/bundles/jsapplibs",
        "~/Scripts/main.js"
    )
    *@
</body>
</html>